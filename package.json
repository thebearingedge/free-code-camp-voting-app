    "@thebearingedge/test-utils": "3.0.2",
    "babel-cli": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-istanbul": "0.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.8.0",
    "eslint": "2.9.0",
    "mocha": "2.4.5",
    "npm-watch": "0.1.4",
    "rimraf": "2.5.2"
  },
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bcrypt-as-promised": "1.1.0",
