{
  "name": "free-code-camp-voting-app",
  "private": true,
  "version": "0.0.0",
  "description": "Free Code Camp - Build a Voting App",
  "main": "index.js",
  "scripts": {
    "cover": "babel-node node_modules/.bin/babel-istanbul cover",
    "cover:api:unit": "npm run -s cover -- --config .api-unit.yml _mocha --",
    "cover:api:e2e": "npm run -s cover -- --config .api-e2e.yml _mocha --",
    "lint": "eslint src/**/*.js",
    "mocha": "mocha --compilers js:babel-register -r babel-polyfill",
    "api:unit": "SLOW_TESTS=true npm run -s mocha -- src/api/unit",
    "api:unit:watch": "npm run -s mocha -- src/api/unit -w -R min",
    "api:unit:cover": "SLOW_TESTS=true npm run -s cover:api:unit -- src/api/unit",
    "api:e2e": "npm run -s mocha -- src/api/e2e",
    "api:e2e:watch": "npm run -s api:e2e -- -w -R min",
    "api:e2e:cover": "npm run -s cover:api:e2e -- src/api/e2e",
    "api:watch": "babel-node src/api",
    "karma": "cd src && karma start --single-run",
    "client:unit": "npm run -s karma",
    "client:unit:watch": "npm run -s karma -- --auto-watch --no-single-run",
    "client:unit:cover": "npm run -s karma -- --reporters='mocha,coverage'",
    "knex": "babel-node node_modules/.bin/knex --cwd src --",
    "db:make": "npm run -s knex migrate:make --",
    "db:up": "npm run -s knex migrate:latest",
    "db:down": "npm run -s knex migrate:rollback",
    "db:cycle": "npm run -s db:down && npm run -s db:up",
    "seed:make": "npm run -s knex seed:make --",
    "seed:run": "npm run -s knex seed:run",
    "build:api": "rimraf build && cd src && babel api/*.js index.js config.js -d ../build",
    "bundle": "browserify src/client/index.js > build/public/bundle.js",
    "bundle:watch": "watchify src/client/index.js -o build/public/bundle.js -vd",
    "build:client": "rimraf build/public && mkdirp build/public && npm run -s bundle",
    "start": "node build",
    "html": "jade src/client/index.jade --pretty --out build/public"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@thebearingedge/test-utils": "4.0.0",
    "babel-cli": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-istanbul": "0.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.9.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "browserify-babel-istanbul": "0.4.0",
    "eslint": "2.11.0",
    "eslint-plugin-react": "5.1.1",
    "jade": "1.11.0",
    "karma": "0.13.22",
    "karma-browserify": "5.0.5",
    "karma-cli": "1.0.0",
    "karma-coverage": "1.0.0",
    "karma-jsdom-launcher": "3.0.0",
    "karma-mocha": "1.0.1",
    "karma-mocha-reporter": "2.0.3",
    "mocha": "2.5.3",
    "npm-watch": "0.1.4",
    "rimraf": "2.5.2",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.1.0",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-polyfill": "6.9.1",
    "bcrypt-as-promised": "1.1.0",
    "bluebird": "3.4.0",
    "body-parser": "1.15.1",
    "deep-clone": "2.0.2",
    "es6-error": "3.0.0",
    "express": "4.13.4",
    "express-async-wrap": "1.0.0",
    "global": "4.3.0",
    "joi": "8.4.0",
    "jsonwebtoken": "7.0.0",
    "knex": "0.11.5",
    "pg": "4.5.5",
    "qs": "6.2.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-redux-form": "0.13.1",
    "react-router": "2.4.1",
    "react-router-redux": "4.0.4",
    "redis": "2.6.0-2",
    "redux": "3.5.2",
    "redux-connect": "2.4.0",
    "redux-thunk": "2.1.0",
    "slug": "0.9.1"
  }
}
