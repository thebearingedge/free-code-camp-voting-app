{
  "name": "free-code-camp-voting-app",
  "private": true,
  "version": "0.0.0",
  "description": "Free Code Camp - Build a Voting App",
  "main": "index.js",
  "scripts": {
    "istanbul": "babel-node node_modules/.bin/babel-istanbul cover",
    "istanbul:api:unit": "npm run -s istanbul -- --config .api-unit.yml _mocha --",
    "istanbul:api:e2e": "npm run -s istanbul -- --config .api-e2e.yml _mocha --",
    "lint": "eslint src/**/*.js",
    "mocha": "mocha --compilers js:babel-register -r babel-polyfill",
    "api:unit": "SLOW_TESTS=true npm run -s mocha -- src/api/unit",
    "api:tdd": "npm run -s mocha -- src/api/unit -w -R min",
    "api:e2e": "SLOW_TESTS=true npm run -s mocha -- src/api/e2e",
    "api:e2e:watch": "npm run -s mocha -- src/api/e2e -w -R min",
    "api:cover:unit": "SLOW_TESTS=true npm run -s istanbul:api:unit -- src/api/unit",
    "api:cover:e2e": "SLOW_TESTS=true npm run -s istanbul:api:e2e -- src/api/e2e",
    "api:watch": "babel-node src/app",
    "knex": "babel-node node_modules/.bin/knex --",
    "db:make": "npm run -s knex migrate:make --",
    "db:up": "npm run -s knex migrate:latest",
    "db:down": "npm run -s knex migrate:rollback",
    "db:cycle": "npm run -s db:down && npm run -s db:up",
    "seed:make": "npm run -s knex seed:make --",
    "seed:run": "npm run -s knex seed:run",
    "dev": "npm-watch"
  },
  "watch": {
    "api:watch": {
      "patterns": [
        "src/app.js",
        "src/api/**.js"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@thebearingedge/test-utils": "3.2.0",
    "babel-cli": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-istanbul": "0.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.8.0",
    "eslint": "2.10.2",
    "mocha": "2.4.5",
    "npm-watch": "0.1.4",
    "rimraf": "2.5.2",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.1.0",
    "tape": "4.5.1"
  },
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "bcrypt-as-promised": "1.1.0",
    "bluebird": "3.3.5",
    "body-parser": "1.15.1",
    "deep-clone": "2.0.2",
    "es6-error": "2.1.1",
    "express": "4.13.4",
    "express-async-wrap": "1.0.0",
    "fecha": "2.1.0",
    "joi": "8.1.0",
    "jsonwebtoken": "6.2.0",
    "knex": "0.11.3",
    "pg": "4.5.5",
    "redis": "2.6.0-2",
    "slug": "0.9.1"
  }
}
