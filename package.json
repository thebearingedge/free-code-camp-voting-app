{
  "name": "free-code-camp-voting-app",
  "private": true,
  "version": "0.0.0",
  "description": "Free Code Camp - Build a Voting App",
  "main": "index.js",
  "scripts": {
    "cover": "babel-node node_modules/.bin/babel-istanbul cover",
    "cover:api:unit": "npm run -s cover -- --config .api-unit.yml _mocha --",
    "cover:api:e2e": "npm run -s cover -- --config .api-e2e.yml _mocha --",
    "cover:client:unit": "npm run -s cover -- --config .client-unit.yml _mocha --",
    "lint": "eslint src/**/*.js",
    "mocha": "mocha --compilers js:babel-register -r babel-polyfill",
    "api:unit": "SLOW_TESTS=true npm run -s mocha -- src/api/unit",
    "api:unit:watch": "npm run -s mocha -- src/api/unit -w -R min",
    "api:cover:unit": "SLOW_TESTS=true npm run -s cover:api:unit -- src/api/unit",
    "api:e2e": "npm run -s mocha -- src/api/e2e",
    "api:e2e:watch": "npm run -s api:e2e -- -w -R min",
    "api:cover:e2e": "npm run -s cover:api:e2e -- src/api/e2e",
    "api:watch": "babel-node src/api",
    "client:unit": "npm run -s mocha -- src/client/unit",
    "client:unit:watch": "npm run -s client:unit -- -w -R min",
    "client:cover:unit": "npm run -s cover:client:unit -- src/client/unit",
    "knex": "babel-node node_modules/.bin/knex --cwd src --",
    "db:make": "npm run -s knex migrate:make --",
    "db:up": "npm run -s knex migrate:latest",
    "db:down": "npm run -s knex migrate:rollback",
    "db:cycle": "npm run -s db:down && npm run -s db:up",
    "seed:make": "npm run -s knex seed:make --",
    "seed:run": "npm run -s knex seed:run",
    "build:api": "rimraf build && cd src && babel api/*.js config.js -d ../build",
    "start:dev": "npm-watch",
    "start": "node build/api"
  },
  "watch": {
    "api:watch": {
      "patterns": [
        "src/api/**.js"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@thebearingedge/test-utils": "3.3.0",
    "babel-cli": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-istanbul": "0.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.9.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "browserify-babel-istanbul": "0.4.0",
    "eslint": "2.10.2",
    "karma": "0.13.22",
    "karma-cli": "1.0.0",
    "karma-coverage": "1.0.0",
    "karma-jsdom-launcher": "3.0.0",
    "karma-mocha": "1.0.1",
    "karma-mocha-reporter": "2.0.3",
    "mocha": "2.4.5",
    "npm-watch": "0.1.4",
    "rimraf": "2.5.2",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-polyfill": "6.9.0",
    "bcrypt-as-promised": "1.1.0",
    "bluebird": "3.4.0",
    "body-parser": "1.15.1",
    "deep-clone": "2.0.2",
    "es6-error": "3.0.0",
    "express": "4.13.4",
    "express-async-wrap": "1.0.0",
    "global": "4.3.0",
    "joi": "8.1.0",
    "jsonwebtoken": "7.0.0",
    "knex": "0.11.3",
    "pg": "4.5.5",
    "qs": "6.2.0",
    "redis": "2.6.0-2",
    "redux": "3.5.2",
    "slug": "0.9.1"
  }
}
